{
  "name": "icu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch server server/app",
    "devp": "pm2 start dev.config.cjs",
    "start": "node server/app NODE_ENV=production",
    "pm2": "pm2 start pm2.config.cjs",
    "db": "node server/db/init",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server --outDir dist/server",
    "preview": "vite preview",
    "eslint:comment": "使用 ESLint 检查并自动修复 src 目录下所有扩展名为 .js 和 .vue 的文件",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix server",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "axios": "^1.2.2",
    "iconv-lite": "^0.6.3",
    "koa": "^2.14.1",
    "koa-body": "^6.0.1",
    "koa-router": "^12.0.0",
    "koa-send": "^5.0.1",
    "md-editor-v3": "^2.10.0",
    "mysql2": "^2.3.3",
    "node-html-parser": "^6.1.4",
    "pinia": "^2.0.33",
    "puppeteer": "^19.6.3",
    "sequelize": "^6.28.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.9.0",
    "jest": "^29.3.1",
    "koa-connect": "^2.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "vite": "^4.0.0"
  }
}
